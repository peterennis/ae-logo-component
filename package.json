{
  "name": "ae-logo-component",
  "version": "0.0.4",
  "description": "Build a component for the [adaept](www.adaept.com) logo",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "~0.9.11",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "~21.1.1",
    "jest": "~21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterennis/ae-logo-component.git"
  },
  "keywords": [
    "logo",
    "component"
  ],
  "author": "adaept",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterennis/ae-logo-component"
  },
  "homepage": "https://github.com/peterennis/ae-logo-component",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
