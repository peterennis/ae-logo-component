{
  "name": "ae-svg-components",
  "version": "0.12.0-0",
  "description": "Build components for [adaept](www.adaept.com)",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "stencil build --dev --watch --serve",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "^0.12.0-0",
    "@stencil/router": "^0.2.5",
    "ionicons": "^4.3.0"
  },
  "devDependencies": {
    "@stencil/postcss": "^0.1.0",
    "@stencil/sass": "^0.1.0",
    "@stencil/utils": "^0.0.4",
    "@types/jest": "~23.3.1",
    "autoprefixer": "^9.1.0",
    "jest": "^23.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterennis/ae-svg-components.git"
  },
  "keywords": [
    "stencil",
    "svg",
    "logo",
    "component"
  ],
  "author": "adaept",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peterennis/ae-svg-components"
  },
  "homepage": "https://github.com/peterennis/ae-svg-components",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
